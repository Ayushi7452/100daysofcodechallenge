//Wave Array.
//In this we use the swapping concept.
class Solution {
    public static void convertToWave(int n, int[] a) {
        // code here
        
        for(int i=0;i<n;i+=2){
        
                int temp=a[i];
                a[i]=a[i+1];
                a[i+1]=temp;
           
        }
    }
}
        
