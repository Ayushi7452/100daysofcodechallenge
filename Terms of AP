import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
	Scanner input=new Scanner(System.in);
        int x=input.nextInt();
        int n;
        
        
        for(n=1;n<=x;n++){
            int d=3*n+2;
            if(d%4==0){
                x++;
                continue;
            }
                System.out.print(d+" ");

            
        }

	}
}
